#include<stdio.h>
main()
{
int limit,num,big=0,small=1000;
printf("enter the limit\n");
scanf("%d",&limit);
printf("enter %d numbers\n",limit);
while(limit>0)
{
scanf("%d",&num);
{
if(num>big)
{big=num;}}
if(num<small)
{small=num;}
limit--;}
printf("biggest number is%d\n",big);
printf("smallest number is%d\n",small);
}
